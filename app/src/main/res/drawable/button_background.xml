<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#233e70"/> <!-- Darker shade for pressed state -->
            <corners android:radius="4dp"/> <!-- 0.25rem in dp -->
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#2b4a84"/> <!-- Background color -->
            <corners android:radius="4dp"/> <!-- 0.25rem in dp -->
        </shape>
    </item>
</selector>
